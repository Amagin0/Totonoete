 <div class='container'>
  <div class='row'>
    <div class="col-sm-12 col-md-8 col-lg-5 px-5 px-sm-0">
      <h2>Log in</h2>
      <%= form_with model: @session, local: true do |f| %>
      <%#= form_with model: @user, url: user_session_path, local: true do |f| %>
        <div class="field">
          <%= f.label :email, "メールアドレス"%><br />
          <%= f.email_field :email, class: "form-control" %>
        </div>
        <div class="field">
          <%= f.label :password, "パスワード" %><br />
          <%= f.password_field :password, autocomplete: "off", class: "form-control" %>
        </div>
        <div class="actions">
          <%= f.submit "ログイン" ,class: 'btn btn-sm btn-primary'%>
        </div>
      <% end %>
    </div>
  </div>
</div>